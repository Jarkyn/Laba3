<body>
<script>
var arr = [12,-1,44,29,35,-2],
aMax;
function getMax(arr) {
var arrLen = arr.length,

      maxS = arr[0];

  for (var i = 0; i < arrLen; i++) {

     if (maxS < arr[i]) {

      maxS = arr[i];

     }}

    return maxS;

  }

  aMax = getMax(arr);

  console.log(aMax); 

  document.write(" en ulken san = " + aMax);

  </script>

</body>

